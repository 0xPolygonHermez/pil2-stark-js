air Simple(int N = 2**6) {

    col witness a[2][3], b[2];

    col fixed B[2]; 
    col fixed A[2][3];


    B[0] = [6...];
    B[1] = [24...];

    A[0][0] = [1...];
    A[0][1] = [2...];
    A[0][2] = [3...];

    A[1][0] = [2...];
    A[1][1] = [3...];
    A[1][2] = [4...];

    a[0][0]*a[0][1]*a[0][2] === b[0]; 
    a[1][0]*a[1][1]*a[1][2] === b[1]; 

    A[0][0]*A[0][1]*A[0][2] === B[0]; 
    A[1][0]*A[1][1]*A[1][2] === B[1]; 
}

Simple(2**6);
